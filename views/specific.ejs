<head>
  <meta content='width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1' name='viewport' />
  <title>Title</title>
  <meta content='text/html; charset=UTF-8' http-equiv='Content-Type' />
  <!-- Chrome, Firefox OS and Opera -->
  <meta content='#f7f7f7' name='theme-color' />
  <!-- Windows Phone -->
  <meta content='#f7f7f7' name='msapplication-navbutton-color' />
  <!-- <meta content='blogger' name='generator' /> -->
  <link href='https://www.captainnk.xyz/favicon.ico' rel='icon' type='image/x-icon' />
  <!-- <link href='https://www.captainnk.xyz/search?updated-max=2020-06-02T16:26:00-07:00&max-results=50' rel='canonical' /> -->
  <!-- Google Fonts -->
  <link rel="stylesheet" type="text/css" href="css/styles.css">

  <!-- <link href='//fonts.googleapis.com/css?family=Open+Sans:400,400i,600,600i,700,700i' media='all' rel='stylesheet' -->
  <!-- type='text/css' /> -->
  <!-- Template Style CSS -->

  <!-- <link href='https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css' rel='stylesheet' /> -->


  <!-- Google Analytics -->
</head>

<body class='index'>
  <!-- Outer Wrapper -->
  <div id='outer-wrapper'>
    <!-- Header Wrapper -->
    <div id='header-wrap'>
      <div class='mobile-menu-wrap'>
        <div class='mobile-menu'></div>
      </div>
      <div class='container row'>
        <div class='header-logo'>
          <div class='main-logo section' id='main-logo' name='Header Logo'>
            <div class='widget Header' data-version='2' id='Header1'>
              <div class='header-widget'>
                <a class='header-brand' href='http://<%= current_url %>'>
                  <img alt='Homelink' data-height='512' data-width='512'
                    src='//1.bp.blogspot.com/-tUTdMF4_iZQ/XszZhYrNH1I/AAAAAAAAAJE/MrxWLnrI-30ERgBIP4ChUBfiT-8L-a0lACK4BGAYYCw/s1600/d78vmes-823c287b-31f7-4b6b-b043-70d8e83a9b2f.png' />
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class='header-menu'>
          <div class='main-menu section' id='main-menu' name='Main Menu'>
            <!-- <div class='widget LinkList' > -->
            <ul id='main-menu-nav' role='menubar'>
              <li><a href='/' role='menuitem'>Home</a></li>
              <li><a href="http://<%= current_url %>/bollywood_movies" role='menuitem'>Bollywood movie</a></li>
              <li><a href='http://<%= current_url %>/bollywood_series' role='menuitem'>
                  Bollywood series</a></li>
              <li><a href='http://<%= current_url %>/hollywood_series' role='menuitem'>
                  Hollywood series</a></li>
              <li><a href='http://<%= current_url %>/hollywood_movies' role='menuitem'>Hollywood movie</a></li>
              <li><a href='https://www.captainnk.xyz/2020/06/privacy-policy.html' role='menuitem'>PRIVACY POLICY</a>
              </li>
            </ul>
            <!-- </div> -->
          </div>
        </div>
        <div id='nav-search'>
          <form action="http://<%= current_url %>/movies" type="get" class='search-form' role='search'>
            <input autocomplete='off' class='search-input' name='title' placeholder='Search this blog' type='search'
              value='' />
            <span class='hide-search' id='hide-search'></span>
          </form>
        </div>
        <span class='show-search' id='show-search'></span>
        <span class='mobile-menu-toggle'></span>
      </div>
    </div>
    <!-- Content Wrapper -->
    <div class='row' id='content-wrapper'>
      <div class='container'>
        <!-- Main Wrapper -->
        <div id='main-wrapper'>
          <div class='main section' id='main' name='Main Posts'>
            <div class='widget Blog' data-version='2' id='Blog1'>
              <div class='blog-posts hfeed index-post-wrap'>

                <%   for(var i=index*9*3;i<(index+1)*27;i++){ %>
                <% if(movie_list[i] == undefined ) continue; %>
                <div class='blog-post hentry index-post'>
                  <div class='post-content' style="background-color: #fff;">
                    <div class='post-image-wrap'>
                      <% if(movie_list[i].href!== undefined) {%>
                      <a class='post-image-link' href="<%= movie_list[0].href  %>">
                        <% } else {%>
                        <a class='post-image-link'
                          href="http://<%= current_url+'/movies?title='+movie_list[0].movie_name  %>">
                          <% } %>
                          <img alt="<%= movie_list[0].title %>" class='post-thumb' src='<%= movie_list[0].src %>' />
                          <div class='post-info'>
                            <div class='post-info-inner'>
                              <h2 class='post-title'>
                                <%= movie_list[0].title %>
                              </h2>
                              <p class='post-snippet'> <%= movie_list[0].discription %> </p>
                            </div>
                          </div>
                        </a>
                        <a class='read-more' href="<%= movie_list[0].href %>">Read more</a>
                    </div>
                  </div>
                </div>

                <% } %>



              </div>
            </div>
          </div>
        </div>
        <div class="blog-pager" id="blog-pager">
          <a class="blog-pager-older-link"
            href="http://<%= current_url + '/' + type+ '?'+'index='+String(parseInt(index)+1) %>"
            id="Blog1_blog-pager-older-link" title="Older Posts">
            Load more posts
          </a>
        </div>
      </div>
    </div>

    <!-- Footer Wrapper -->
    

  </div>
  <!-- Main Scripts -->
  <script src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js' type='text/javascript'></script>
  <!-- Theme Functions JS -->

  <!-- Facebook SDK -->
  <!-- <script type='text/javascript'>
    //<![CDATA[
    (function (d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.0';
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
//]]>
  </script> -->
  <!-- Overlay and Back To Top -->
  <div class='overlay'></div>
  <div class='back-top' title='Back to Top' id="move_up"><i class="far fa-badge-check"></i></div>

  <!-- <script type="text/javascript" src="https://www.blogger.com/static/v1/widgets/289343945-widgets.js"></script> -->

</body>
<script>
  $("#show-search").click(function () { $('#nav-search').css("display", "block"); });
  $("#hide-search").click(function () { $('#nav-search').css("display", "none"); });
  $(document).scroll(function () {
    var y = $(this).scrollTop();
    if (y > 800) {
      $('#move_up').css("display", "block");
      $('#move_up').fadeIn();
    } else {
      $('#move_up').fadeOut();
    }
  });
  $('#move_up').click(() => {
    $(document).scrollTop(0);
  })
</script>

</html>